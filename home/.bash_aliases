# safety
alias cp='cp -i'
alias mv='mv -i'
alias yes='echo "yes! yes! yes!"'

# dir listing and changing
alias l='ls -alh'
alias la='ls -A'
alias ll='ls -alF'
alias ltr='ls -ltr'
alias t='l -rt'
alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias d=dirs
alias pd=pushd
alias po=popd

# Shortcuts
alias dl='cd ~/Downloads'
alias wl='cd ~/workspace; ls
alias c=clear
alias f=finger
alias h='history'
alias h10='history 10'
alias j='jobs -l'
alias m=less
alias ns='nslookup -type=any'
alias psg='/bin/ps auxww | grep'
alias purge='/bin/rm -f \#*\# *~ .*~ *% .*% core'

# git
alias av='git branch -av'
alias suno='git status -s -uno'
alias rv='git remote -v'

